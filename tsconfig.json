{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/messages/page.tsxbbt",
    "lib/websocket-server.js",
    "app/messages/page.bcp",
    "lib/websocket-server.js",
    "app/messages/page.text",
    ".next/dev/types/**/*.ts"
, "lib/models/user.ts.bp", "lib/firebase/config.ts.bp", "types/next-auth.d.ts.b", "lib/auth.ts.bp0", "lib/auth.ts.B.0", "lib/models/user.ts.bp.0"  ],
  "exclude": [
    "node_modules"
  ]
}
